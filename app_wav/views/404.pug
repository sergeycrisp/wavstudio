body
    header(style='position: fixed; width: 100%; z-index: 999')
      .uk-navbar-container.tm-navbar-container(uk-sticky='cls-active: tm-navbar-container-fixed;')
        .uk-container(uk-navbar='')
          .uk-navbar-left
            button.uk-navbar-toggle(class='uk-hidden@m', uk-toggle='target: #nav-offcanvas', uk-navbar-toggle-icon='')
            a.uk-navbar-item.uk-logo(href='index.html')
              img(src='./dest/images/logo.png', width='90', height='32', alt='Logo', style='filter: opacity(75%)')
            |             
            nav(class='uk-visible@m')
              ul.uk-navbar-nav
                li
                  a(href='blog.html') Blog
                |                 
                li
                  a(href='music.html') Music
                |                 
                li
                  a(href='services.html') Services
                |                 
                li
                  a(href='about.html') About
                |                 
                li
                  a(href='contacts.html') Contacts
          |           
          .uk-navbar-right
            a.uk-navbar-item.uk-link-muted.tm-navbar-button(href='account.html', uk-icon='icon: user; ratio:2')
    |     
    main
      section.uk-section.uk-section-small(style='padding-top: 80px; padding-bottom: 160px;')
        .uk-container
          .uk-text-center
            h1.uk-heading-hero 404
            |             
            .uk-text-lead Page not found
            |             
            .uk-margin-top
              | Looks like the page you&apos;re trying to visit doesn&apos;t exist.
              br
              a(href='index.html') Go back to Homepage
      |       
      section.uk-section.uk-section-default.uk-section-small
        .uk-container
          div(uk-slider='')
            ul.uk-slider-items.uk-child-width-1-1.uk-grid(class='uk-child-width-1-2@s uk-child-width-1-5@m')
              li
                .uk-grid-small.uk-flex-center(class='uk-flex-left@s', uk-grid='')
                  div
                    span(uk-icon='icon: star; ratio: 2.5;')
                  |                   
                  .uk-text-center(class='uk-text-left@s uk-width-expand@s')
                    div &Lcy;&yucy;&bcy;&ocy;
                    |                     
                    .uk-text-meta &Kcy;&rcy;&acy;&scy;&icy;&vcy;&ocy;
              |               
              li
                .uk-grid-small.uk-flex-center(class='uk-flex-left@s', uk-grid='')
                  div
                    span(uk-icon='icon: tag; ratio: 2.5;')
                  |                   
                  .uk-text-center(class='uk-text-left@s uk-width-expand@s')
                    div &Dcy;&ocy;&rcy;&ocy;&gcy;&ocy;
                    |                     
