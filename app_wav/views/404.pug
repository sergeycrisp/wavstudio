                .uk-grid-small.uk-flex-center(class='uk-flex-left@s', uk-grid='')
                  div
                    span(uk-icon='icon: database; ratio: 2.5;')
                  |                   
                  .uk-text-center(class='uk-text-left@s uk-width-expand@s')
                    div &Bcy;&ocy;&gcy;&acy;&tcy;&ocy;
                    |                     
                    .uk-text-meta &Dcy;&iecy;&ncy;&softcy;&gcy;&icy; &mcy;&ncy;&ocy;&gcy;&ocy;
              |               
              li
                .uk-grid-small.uk-flex-center(class='uk-flex-left@s', uk-grid='')
                  div
                    span(uk-icon='icon: comments; ratio: 2.5;')
                  |                   
                  .uk-text-center(class='uk-text-left@s uk-width-expand@s')
                    div &Pcy;&ocy;&chcy;&icy;&rcy;&icy;&kcy;&acy;&iecy;&mcy;
                    |                     
                    .uk-text-meta &Pcy;&ocy;&dcy;&dcy;&iecy;&rcy;&zhcy;&kcy;&acy;
              |               
              li
                .uk-grid-small.uk-flex-center(class='uk-flex-left@s', uk-grid='')
                  div
                    span(uk-icon='icon: happy; ratio: 2.5;')
                  |                   
                  .uk-text-center(class='uk-text-left@s uk-width-expand@s')
